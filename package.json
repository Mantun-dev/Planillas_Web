        "express-handlebars": "^5.3.2",
        "feather-icons": "^4.28.0",
        "handlebars": "^4.7.7",
        "laravel-mix": "^6.0.25",
        "nodemon": "^2.0.12",
        "nunjucks": "^3.2.3",
