                                    <th>Área</th>
                                    <th>Acciones</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each employees}}
                                <tr>
                                    <td>{{employeeDNI}}</td>
                                    <td>{{employeeNumber}}</td>
                                    <td>{{employeeFullName}}</td>
                                    <td>{{employeePhone}}</td>
                                    <td>{{townName}}</td>
                                    <td>{{areaDescription}}</td>
                                    <td>
