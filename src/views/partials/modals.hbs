<!-- Modal -->
<div class="modal fade" id="addEmployeed" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
    aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title" id="exampleModalCenterTitle">Nuevo empleado
                </h1>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="margin-left: 600px;">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="form-group col-sm">
                        <label for="recipient-name" class="col-form-label">Identidad:</label>
                        <input type="text" class="form-control" id="dniEmployee"
                            onkeypress="return validateNumber(event)">
                    </div>
                    <div class="form-group col-sm">
                        <label for="recipient-name" class="col-form-label">Número de empleado:</label>
                        <input type="text" class="form-control" id="numberEmployee">
                    </div>
                    <div class="form-group col-sm">
                        <label for="recipient-name" class="col-form-label">Nombre:</label>
                        <input type="text" class="form-control" id="nameEmployee" required>
                    </div>

                </div>
                <div class="row">
                    <div class="form-group col-sm">
                        <label for="recipient-name" class="col-form-label">Fecha
                            de nacimiento:</label>
                        <input type="date" class="form-control" id="dateEmployee" required>
                    </div>
                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label">Teléfono:</label>
                        <input type="text" class="form-control" id="phoneEmployee"
                            onkeypress="return validateNumber(event)">
                    </div>
                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label">Fecha
                            de ingreso:</label>
                        <input type="date" class="form-control" id="dateIn">
                    </div>

                </div>
                <div class="row">
                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label">No de
                            cuenta BAC:</label>

                        <input type="text" class="form-control" id="account" onkeypress="return validateNumber(event)">
                    </div>
                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label">Salario
                            mensual:</label>
                        <input type="text" class="form-control" id="salary" onkeypress="return validateNumber(event)">
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label form-group">Entidad:</label>
                        <select id="entity" style="width:150px;" placeholder="Buscar..." required>
                            <option value="">Seleccione...</option>
                            {{#if entities}}
                            {{#each entities}}
                            <option value="{{entityId}}">{{entityName}}</option>
                            {{/each}}
                            {{else}}

                            <thead>
                                <tr class="text-center">
                                    <th>No hay registros</th>
                                </tr>
                            </thead>
                            {{/if}}
                        </select>
                    </div>

                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label form-group">Ciudad:</label>
                        <select id="town" style="width:150px;" placeholder="Buscar..." required>
                            <option value="">Seleccione...</option>
                            {{#if towns}}
                            {{#each towns}}
                            <option value="{{townId}}">{{townName}}</option>
                            {{/each}}
                            {{else}}

                            <thead>
                                <tr class="text-center">
                                    <th>No hay registros</th>
                                </tr>
                            </thead>
                            {{/if}}
                        </select>
                    </div>

                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label form-group">Area:</label>
                        <select id="area" style="width:150px;" placeholder="Buscar..." required>
                            <option value="">Seleccione...</option>
                            {{#if area}}
                            {{#each area}}
                            <option value="{{areaId}}">{{areaDescription}}</option>
                            {{/each}}
                            {{else}}

                            <thead>
                                <tr class="text-center">
                                    <th>No hay registros</th>
                                </tr>
                            </thead>
                            {{/if}}
                        </select>
                    </div>

                    <div class="form-group col-sm">
                        <label for="message-text" class="col-form-label form-group">Tipo planilla:</label>
                        <select id="payrrolls" style="width:150px;" placeholder="Buscar..." required>
                            <option value="">Seleccione...</option>
                            {{#if payrrolls}}
                            {{#each payrrolls}}
                            <option value="{{typeId}}">{{typeDescription}}</option>
                            {{/each}}
                            {{else}}

                            <thead>
                                <tr class="text-center">
                                    <th>No hay registros</th>
                                </tr>
                            </thead>
                            {{/if}}
                        </select>
                    </div>

                </div>

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                <button type="submit" class="btn btn-primary" onclick="addEmployeed()">Guardar</button>

            </div>

        </div>
    </div>
</div>

