                            <input type="date" class="form-control" id="dateStart" style="width: 20vh;">
                        </div>
                        <div class="form-group col">
                            <label for="message-text" class="col-form-label">Fecha
                                de fin:</label>
                            <input type="date" class="form-control" id="dateEnd" style="width: 20vh;">
                        </div>
                        <div class="form-group col-sm">
                            <label for="message-text" class="col-form-label form-group">Entidad:</label>
                            <select id="entityId" style="width:150px;" placeholder="Buscar..." required>
                                <option value="">Seleccione...</option>
                                {{#if entities}}
                                {{#each entities}}
                                <option value="{{entityId}}">{{entityName}}</option>
                                {{/each}}
                                {{else}}

                                <thead>
                                    <tr class="text-center">
                                        <th>No hay registros</th>
                                    </tr>
                                </thead>
                                {{/if}}
                            </select>
                        </div>
                    </div>
                </div>
            </div>
